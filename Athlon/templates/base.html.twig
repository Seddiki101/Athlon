<!DOCTYPE html>
<html lang="zxx">
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <meta name="description" content="Activitar Template">
        <meta name="keywords" content="Activitar, unica, creative, html">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta http-equiv="X-UA-Compatible" content="ie=edge">
        <link rel="icon" href="{{ asset("/F/assets/data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>") }}">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            <!-- Css Styles -->
            <link rel="stylesheet" href="{{ asset("/F/assets/css/bootstrap.min.css") }}" type="text/css">

            <link rel="stylesheet" href="{{ asset("/F/assets/css/font-awesome.min.css") }}" type="text/css">
            <link rel="stylesheet" href="{{ asset("/F/assets/css/elegant-icons.css") }}" type="text/css">
            <link rel="stylesheet" href="{{ asset("/F/assets/css/nice-select.css") }}" type="text/css">
            <link rel="stylesheet" href="{{ asset("/F/assets/css/owl.carousel.min.css") }}" type="text/css">
            <link rel="stylesheet" href="{{ asset("/F/assets/css/magnific-popup.css") }}" type="text/css">
            <link rel="stylesheet" href="{{ asset("/F/assets/css/slicknav.min.css") }}" type="text/css">
            <link rel="stylesheet" href="{{ asset("/F/assets/css/style.css") }}" type="text/css">
        {% endblock %}

        {% block javascripts %}
            <script src="{{ asset("/F/assets/js/jquery-3.3.1.min.js") }}"></script>
            <script src="{{ asset("/F/assets/js/bootstrap.min.js") }}"></script>
            <script src="{{ asset("/F/assets/js/jquery.magnific-popup.min.js") }}"></script>
            <script src="{{ asset("/F/assets/js/mixitup.min.js") }}"></script>
            <script src="{{ asset("/F/assets/js/jquery.nice-select.min.js") }}"></script>
            <script src="{{ asset("/F/assets/js/jquery.slicknav.js") }}"></script>
            <script src="{{ asset("/F/assets/js/owl.carousel.min.js") }}"></script>
            <script src="{{ asset("/F/assets/js/masonry.pkgd.min.js") }}"></script>
            <script src="{{ asset("/F/assets/js/main.js") }}"></script>
        {% endblock %}
    </head>
    <body>
    <!-- Page Preloder -->
    <div id="preloder">
        <div class="loader"></div>
    </div>

    <!-- Header Section Begin -->
    <header class="header-section header-normal">
        <div class="container-fluid">
            <div class="logo">
                <a href="./index.html">
                    <img src="img/logo.png" alt="">
                </a>
            </div>

            <div class="top-social">
                <a href="#">  <i class="fa fa-user"></i> Sign up/Sign in  </a>
            </div>
            <div class="top-social">
                <a href=".">  <i class="fa fa-cart-plus"></i> Cart  </a>
            </div>
            <div class="container">
                <div class="nav-menu">
                    <nav class="mainmenu mobile-menu">
                        <ul>
                            <li><a href="./index.html">Home</a></li>
                            <li><a href="./about-us.html">About us</a></li>
                            <li><a href="./schedule.html">Schedule</a></li>
                            <li class="active"><a href="./gallery.html">Gallery</a></li>
                            <li><a href="./blog.html">Blog</a>
                                <ul class="dropdown">
                                    <li><a href="./about-us.html">About Us</a></li>
                                    <li><a href="./blog-single.html">Blog Details</a></li>
                                </ul>
                            </li>
                            <li><a href="./contact.html">Contacts</a></li>
                        </ul>
                    </nav>
                </div>
            </div>
            <div id="mobile-menu-wrap"></div>
        </div>
    </header>

        {% block body %}{% endblock %}
    </body>
</html>
